-- Noclip Section - Begin
local Section = Tab:AddSection({
	Name = "NoClip"
})
-- Noclip Section - End

-- NoClip On - Begin
Tab:AddButton({
	Name = "On",
	Callback = function()

		local Char = Plr.Character:GetChildren()
		for i, v in pairs(Char) do
			   if v.ClassName == "MeshPart" then
			   v.CanCollide = false
			   
			   
				elseif v.ClassName == "Hat" then
				   local Hats = v:GetChildren()
				   for i, v in pairs(Hats) do
					   if v.ClassName == "MeshPart" then
						   v.CanCollide = false
					   end
					   
				   end
				   
				for i, v in pairs(Char) do
					   if v.ClassName == "Part" then
						   v.CanCollide = false
					   end
				end
				end
			   
		   
		end


		
  	end  -- Function End  
})
-- NoClip On - End

-- NoClip Off - Begin
Tab:AddButton({
	Name = "Off",
	Callback = function()

		local Char = Plr.Character:GetChildren()
 for i, v in pairs(Char) do
		if v.ClassName == "MeshPart" then
		v.CanCollide = true
		
		
		 elseif v.ClassName == "Hat" then
			local Hats = v:GetChildren()
			for i, v in pairs(Hats) do
				if v.ClassName == "MeshPart" then
					v.CanCollide = true
				end
				
			end
			
		 for i, v in pairs(Char) do
				if v.ClassName == "Part" then
					v.CanCollide = true
				end
		 end
		 end
		
	
 end





  	end -- Function End   
})
-- NoClip Off - End

